{
  "name": "juicebar",
  "version": "1.0.0",
  "description": "A Vue.js project, from 'vue-typescript-bulma-template'",
  "author": "Thomas Paradis",
  "scripts": {
    "css-build": "node-sass --omit-source-map-url src/assets/sass/bulma.sass src/assets/css/bulma.css & node-sass --omit-source-map-url src/assets/scss/core.scss src/assets/css/core.css",
    "css-watch": "npm run css-build -- --watch",
    "dev": "concurrently --kill-others npm:dev-wp npm:css-watch",
    "build": "rm -rf docs/* && npx webpack --progress --config webpack.config.prod.js",
    "dev-wp": "npx webpack-dev-server --progress --config webpack.config.dev.js",
    "start": "npm run dev"
  },
  "dependencies": {
    "jquery": "^3.4.1",
    "slick-carousel": "^1.8.1",
    "webp-loader": "^0.6.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome": "^1.1.6",
    "@fortawesome/fontawesome-free-solid": "^5.0.11",
    "concurrently": "^5.1.0",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "ignore-loader": "^0.1.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.9.0",
    "npm": "^6.0.0",
    "optimize-css-assets-webpack-plugin": "^4.0.1",
    "prerender-spa-plugin": "^3.1.0",
    "sass-loader": "^7.0.1",
    "script-ext-html-webpack-plugin": "^2.0.1",
    "style-loader": "^0.21.0",
    "ts-loader": "^4.2.0",
    "typescript": "^2.8.3",
    "url-loader": "^1.0.1",
    "vue": "^2.5.16",
    "vue-class-component": "^6.2.0",
    "vue-loader": "^15.0.5",
    "vue-property-decorator": "^6.0.0",
    "vue-router": "^3.0.1",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.2",
    "webpack-dev-server": "^3.1.3"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  }
}